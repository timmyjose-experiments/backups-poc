{
  "expo": {
    "name": "backups-poc",
    "slug": "backups-poc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timmyjose.backupspoc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timmyjose.backupspoc"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "be1e9e9b-48ef-4066-88da-afdfb32d8fde"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/be1e9e9b-48ef-4066-88da-afdfb32d8fde"
    },
    "plugins": [
      [
        "./src/plugins/withAndroidApplicationAttributes.js",
        {
          "android:backupAgent": "expo.modules.androidkvbackupagent.PayyBackupAgent",
          "android:allowBackup": "true",
          "android:restoreAnyVersion": "true"
        }
      ],

      [
        "react-native-cloud-storage",
        {
          "iCloudContainerEnvironment": "Production",
          "iCloudContainerIdentifier": "iCloud.com.timmyjose.backupspoc"
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.4"
          }
        }
      ]
    ]
  }
}
